{
  "name": "@storehouse/redis-om",
  "version": "1.0.0",
  "description": "Redis OM manager for @storehouse/core",
  "main": "lib/index.js",
  "scripts": {
    "doc": "typedoc",
    "build": "tsc",
    "lint": "eslint .",
    "test": "kaukau --require ts-node/register --config kaukau-src.config.mjs",
    "test:lib": "kaukau --config kaukau-lib.config.mjs",
    "format": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --write ./src",
    "format:check": "cross-env NODE_OPTIONS=--experimental-strip-types prettier --check ./src",
    "format:staged": "cross-env NODE_OPTIONS=--experimental-strip-types lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisiwu/storehouse-redis-om.git"
  },
  "keywords": [
    "storehouse",
    "redis-om"
  ],
  "author": "demingongo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kisiwu/storehouse-redis-om/issues"
  },
  "homepage": "https://kisiwu.github.io/storehouse/redis-om/latest/",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "chai": "^6.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "globals": "^17.3.0",
    "kaukau": "^4.2.1",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "dependencies": {
    "@novice1/logger": "^1.6.1",
    "@storehouse/core": "^2.1.0",
    "redis": "^5.10.0",
    "redis-om": "^0.4.7",
    "tslib": "^2.8.1"
  }
}